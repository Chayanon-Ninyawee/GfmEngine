{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build TestGfmEngine (Debug)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "@CMAKE_BINARY_DIR@",
                "--config",
                "Debug"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "Build TestGfmEngine (Release)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "@CMAKE_BINARY_DIR@",
                "--config",
                "Release"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "Build TestGfmEngine With Docker (Release)",
            "type": "shell",
            "command": "docker build -t gfmengine-builder @CMAKE_SOURCE_DIR@ && docker image prune -f && docker run --name temp-container gfmengine-builder && rm -rf @CMAKE_SOURCE_DIR@/ubuntu20/build && mkdir @CMAKE_SOURCE_DIR@/ubuntu20 || true && docker cp temp-container:/app/build @CMAKE_SOURCE_DIR@/ubuntu20/build && docker rm temp-container",
            "problemMatcher": []
        }
    ]
}