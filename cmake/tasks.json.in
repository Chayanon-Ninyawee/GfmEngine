{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build TestGfmEngine (Debug)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build", "@CMAKE_BINARY_DIR@",
                "--config", "Debug"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"],
            "dependsOn": ["Configure CMake (Debug)"]
        },
        {
            "label": "Build TestGfmEngine (Release)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build", "@CMAKE_BINARY_DIR@",
                "--config", "Release"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": ["$gcc"],
            "dependsOn": ["Configure CMake (Release)"]
        }
    ]
}
