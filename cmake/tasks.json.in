{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build TestGfmEngine (Debug)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "@CMAKE_BINARY_DIR@",
                "--config",
                "Debug"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "Build TestGfmEngine (Release)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "@CMAKE_BINARY_DIR@",
                "--config",
                "Release"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "Build TestGfmEngine With Docker (Release)",
            "type": "shell",
            "command": "@CMAKE_SOURCE_DIR@/build-ubuntu20-docker.sh",
            "problemMatcher": []
        }
    ]
}